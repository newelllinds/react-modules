yProjects2/react-modules/src/index.js",[],["87","88"],"/Applications/ElevenFiftyProjects2/react-modules/src/App.js",[],"/Applications/ElevenFiftyProjects2/react-modules/src/reportWebVitals.js",[],"/Applications/ElevenFiftyProjects2/react-modules/src/components/site/Footer.js",[],"/Applications/ElevenFiftyProjects2/react-modules/src/components/site/Sidebar.js",[],"/Applications/ElevenFiftyProjects2/react-modules/src/components/site/Header.js",[],"/Applications/ElevenFiftyProjects2/react-modules/src/components/site/Home.js",[],"/Applications/ElevenFiftyProjects2/react-modules/src/components/site/Resources.js",[],"/Applications/ElevenFiftyProjects2/react-modules/src/components/concepts/FunctionalComponentDemo.js",[],"/Applications/ElevenFiftyProjects2/react-modules/src/components/concepts/JSXRules.js",["89"],"import React from 'react';\n\nconst JSXRules = () => {\n    return (\n        <div className = \"main\">\n            <div className = \"mainDiv\">\n                <h1 className=\"section-title\">JSX</h1>\n                <dl>\n                    <dt>Resembles HTML</dt>\n                    <dd>It's not. It's actually closer to JavaScript.</dd>\n                    <dt>React Elements</dt>\n                    <dd>These are used to construct and update the DOM, or what you see on the screen.</dd>\n                    <dt>Not Required</dt>\n                    <dd>You can write in vanilla JS, but most sane people use JSX.</dd>\n                </dl>\n                <hr />\n            </div>\n            <NormalComponent />\n            <CreateElementComponent />\n        </div>\n    );\n};\n\nexport default JSXRules;\n\nconst NormalComponent = () => {\n    return (\n        <div style={{ border: '2px solid black' }}>\n            <h1>Normal Functional Component</h1>\n            <p>This was constructed with JSX in the return.</p>\n            <img src=\"https://pbs.twimg.com/media/DOzL82mXkAA0zFs.jpg\" />\n        </div>\n    );\n};\n\nconst CreateElementComponent = () => {\n    return (\n        React.createElement('div', { style: { border: '2px solid black' } },\n            React.createElement('h1', null, 'CreateElement Component'),\n            React.createElement('p', null, 'This was constructed with CreateElement calls in the return.'),\n            React.createElement('img', { src: 'https://cdn-images-1.medium.com/max/1200/1*jJZHFQmhkq_7ohn18KrMhA.png'}, null)\n        )\n    );\n};","/Applications/ElevenFiftyProjects2/react-modules/src/components/concepts/State.js",["90"],"import React, {useState} from 'react';\n\nconst State = () => {\n    return (\n        <div className=\"main\">\n            <div className=\"mainDiv\">\n                <ul>\n                    <dt>useState is Unique to Functional Components</dt>\n                    <dd>Class components have separate means of using and changing state.</dd>\n                    <dt>useState Uses Array Destructuring</dt>\n                    <dd>useState proves a state variable and a setState function</dd>\n                    <dt>useState is a Hook</dt>\n                    <dd>useState is a hook baked into React</dd>\n                    <dt>Triggers Re-renders</dt>\n                    <dd>Like with props changes, changing the state of a component re-renders the whole component.</dd>\n                </ul>\n            </div>\n            <StateExample />\n        </div>\n    );\n};\n\nfunction StateExample() {\n    const [text, setText] = useState('');\n    const [likeNum, setLikeNum] = useState(0);\n\n    return(\n        <div>\n            <input value={text} onChange={e => setText(e.target.value)}/>\n            <br />\n            <img style={{ width: '100px', height: '100px' }} src=\"https://upload.wikimedia.org/wikipedia/commons/1/13/Facebook_like_thumb.png\" onClick={e => setLikeNum(likeNum + 1)} />\n            <span>{likeNum}</span>\n        </div>\n    );\n};\n\nexport default State;","/Applications/ElevenFiftyProjects2/react-modules/src/components/concepts/Effects.js",[],"/Applications/ElevenFiftyProjects2/react-modules/src/components/concepts/PropsDemo.js",["91","92","93"],"import React, { useState } from 'react';\nimport PropTypes from 'prop-types';\n\nconst PropsDemo = () => {\n    const [ color, setColor ] = useState('white');\n    const [ backgroundColor, setBackgroundColor ] = useState('purple');\n    const [ borderRadius, setBorderRadius ] = useState('5px');\n    const [ borderStyle, setBorderStyle ] = useState('dashed');\n    const [ display, setDisplay ] = useState ('inline-block');\n    const [ width, setWidth ] = useState('350px');\n    const [ textAlign, setTextAlign ] = useState('center');\n\n    let styles = {\n        color: color,\n        backgroundColor: backgroundColor,\n        borderRadius: borderRadius,\n        borderStyle: borderStyle,\n        display: display,\n        width: width,\n        textAlign: textAlign\n    }\n\n    const toggleColor = () => {\n        color === 'white' ? setColor('pink') : setColor('white');\n    }\n    const backgroundColorFunction = () => {\n        backgroundColor === 'green' ? setBackgroundColor('red') : setBackgroundColor('green');\n    }\n    const borderRadiusFunction = () => {\n        borderRadius === '5px' ? setBorderRadius('40px') : setBorderRadius('5px');\n    }\n    const borderStyleFunction = () => {\n        borderStyle === 'dotted' ? setBorderStyle('solid') : setBorderStyle('dotted');\n    }\n\n    return (\n        <div className=\"main\">\n            <div className=\"mainDiv\">\n                <div style={styles}>\n                <FunctionalComponent string=\"will this display?\" function={toggleColor} selectedStyle={ color }/>\n                <FunctionalComponent string=\"yes, it will\" function={backgroundColorFunction} selectedStyle={ backgroundColor }/>\n                <FunctionalComponent string=\"because I...\" function={borderRadiusFunction} selectedStyle={ borderRadius }/>\n                <FunctionalComponent string=\"AM AWESOME!\" function={borderStyleFunction} selectedStyle= {borderStyle }/>\n                </div>\n            </div>\n        </div>\n    );\n};\n\nconst FunctionalComponent = (props) => {\n    return (\n        <div>\n            <p>{props.string}</p>\n            <button onClick={props.function}>Press Me!</button>\n            <TinyComponent selectedStyle={ props.selectedStyle } />\n        </div>\n    );\n}\n\nconst TinyComponent = (props) => {\n    return (\n        <div>\n            <p>The current style is : { props.selectedStyle }</p>\n        </div>\n    )\n}\n\nFunctionalComponent.defaultProps = {\n    string: 'This is wild!',\n    function: () => console.log('Can I see this in my dev tools?'),\n    selectedStyle: 'What style??'\n}\n\nFunctionalComponent.propTypes = {\n    string: PropTypes.string.isRequired,\n    funtion: PropTypes.func.isRequired,\n    selectedStyle: PropTypes.string.isRequired\n}\n\nexport default PropsDemo;","/Applications/ElevenFiftyProjects2/react-modules/src/components/concepts/Hooks.js",["94"],"import React, {useState, useEffect} from 'react';\n\nconst Hooks = () => {\n    const [query, setQuery] = useState('');\n    const [results, setResults] = useState({});\n\n    const fetcher = () => {\n        fetch(`https://swapi.dev/api/people/${query}`)\n        .then(res => res.json())\n        .then(json => {\n            console.log(json)\n            setResults(json);\n        })\n    }\n\n    return(\n        <div className=\"main\">\n            <div className=\"mainDiv\">\n                <input value={query} onChange={e => setQuery(e.target.value)} placeholder=\"enter your sw character number\"/>\n                <button onClick={() => fetcher()}>Click for Character!</button>\n                {results ? <h2>{results.name}</h2> : <div></div>}\n            </div>\n        </div>\n    );\n};\n\nconst Hooks2 = () => {\n    const [results, queryNum, setQueryNum] = useNumHook('')\n    const [clicks, setClicks] = useClicks(0);\n\n    return(\n        <div className = \"main\">\n            <div className = \"mainDiv\">\n                <h3>Enter a number below to see a number fact</h3>\n                <input value={queryNum} onChange={e => setQueryNum(e.target.value)} placeholder=\"enter a number\"/>\n                {results ? <h2>{results}</h2> : <div></div>}\n                <button onClick = {() => setClicks(clicks + 1)}>Click to update document title</button>\n            </div>\n        </div>\n    );\n};\n\nconst useNumHook = (num) => {\n    const [queryNum, setQueryNum] = useState(num);\n    const [results, setResults] = useState('');\n\n    useEffect(() => {\n        if (queryNum !== ''){\n            fetch(`http://numbersapi.com/${queryNum}`)\n            .then(res => res.text())\n            .then(json => {\n                setResults(json);\n                console.log(json)\n            });\n        };\n    }, [queryNum]);\n\n    return [results, queryNum, setQueryNum];\n};\n\nconst useClicks = (initCount) => {\n    const [clicks, setClicks] = useState(initCount);\n\n    useEffect(() => {\n        document.title = `You have clicked ${clicks} times`;\n    }, [clicks])\n    return [clicks, setClicks]\n}\n\nexport default Hooks2;","/Applications/ElevenFiftyProjects2/react-modules/src/components/apps/timer-apps/TimePiecesApp.js",[],"/Applications/ElevenFiftyProjects2/react-modules/src/components/apps/timer-apps/TimerApp.js",[],"/Applications/ElevenFiftyProjects2/react-modules/src/components/apps/timer-apps/ClockApp.js",[],"/Applications/ElevenFiftyProjects2/react-modules/src/components/apps/timer-apps/StopWatchApp.js",[],"/Applications/ElevenFiftyProjects2/react-modules/src/components/apps/nyt-app/NytApp.js",[],"/Applications/ElevenFiftyProjects2/react-modules/src/components/apps/nyt-app/NytResults.js",[],{"ruleId":"95","replacedBy":"96"},{"ruleId":"97","replacedBy":"98"},{"ruleId":"99","severity":1,"message":"100","line":31,"column":13,"nodeType":"101","endLine":31,"endColumn":74},{"ruleId"